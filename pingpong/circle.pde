class Circle {

  int posX;
  int posY;
  int xSpeed;
  int ySpeed;
  int diameter;

  public Circle(

    int posX,
    int posY,
    int xSpeed,
    int ySpeed,
    int diameter

    ) {
    this.posX = posX;
    this.posY = posY;
    this.xSpeed = xSpeed;
    this.ySpeed = ySpeed;
    this.diameter = diameter;
  };
};
